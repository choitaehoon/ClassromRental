    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.lecture.info.SurveyInfo">
	<insert id="insert">
		INSERT INTO SURVEY(QUESTION1,QUESTION2,QUESTION3,QUESTION4,QUESTION5)
